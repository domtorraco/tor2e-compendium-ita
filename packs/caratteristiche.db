{"_id":"JtHFeco12VTSWfd8","name":"Onorevole","type":"trait","img":"systems/tor2e/assets/images/icons/distinctive_feature.png","effects":[],"flags":{"cf":{"id":"temp_ma8pi2fms8"}},"system":{"description":{"value":"<div title=\"Page 70\">\n<div>\n<div>\n<p>L&rsquo;Eroe crede nell&rsquo;agire con correttezza e nel fare sempre ciò che è moralmente giusto.</p>\n</div>\n</div>\n</div>","type":"String","label":"tor2e.common.description"},"group":{"value":"distinctiveFeature","type":"String","label":"tor2e.traits.details.traitGroup"}},"_stats":{"systemId":"tor2e","systemVersion":"2.0.5","coreVersion":"10.290","createdTime":1669985426160,"modifiedTime":1669985445677,"lastModifiedBy":"Gr9xC9PLRip2tzhG"},"folder":null,"sort":0,"ownership":{"default":0,"Gr9xC9PLRip2tzhG":3}}

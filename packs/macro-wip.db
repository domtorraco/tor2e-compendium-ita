{"name":"Incontri Casuali","type":"script","author":"Gr9xC9PLRip2tzhG","img":"systems/tor2e/assets/images/tengwar.png","scope":"global","command":"let message;\nasync function printMessage(table) {\n   let roll = await table.roll()\n   let result = roll.results[0];\n   let chatData = {\n      content: `<div style=\"text-align:center\">${result.data.text}</div>`,\n      whisper: [game.user],\n    };\n   ChatMessage.create(chatData, {});\n\n }\n\n let dType = new Dialog({\n   title: \"Incontri Casuali\",\n   content: '<div style=\"text-align:center;font-size:24px;\"><h3>dove ti trovi?<\\h3><h2><\\h2><\\div>',\n   buttons: {\n      button1: {\n         label: \"Cavallino Rampante\",\n         callback: () => {\n            message = \"Cavallino Rampante\";\n            printMessage(\n               game.tables.contents\n                  .find((t) => t.name === \"Incontro-Cavallino\")\n            );\n         },\n      },\n      button2: {\n         label: \"Bosco di Cet\",\n         callback: () => {\n            message = \"Bosco di Cet\";\n            printMessage(\n               game.tables.contents\n                  .find((t) => t.name === \"Incontro-Cet\")\n            );\n         },\n      },\n      button3: {\n         label: \"Via Est\",\n         callback: () => {\n            message = \"Via Est\";\n            printMessage(\n               game.tables.contents\n                  .find((t) => t.name === \"Incontro-ViaEst\")\n            );\n         },\n      },\n      button4: {\n         label: \"Verdevia\",\n         callback: () => {\n            message = \"Verdevia\";\n            printMessage(\n               game.tables.contents\n                  .find((t) => t.name === \"Incontro-Verdevia\")\n            );\n         },\n      },\n      button5: {\n         label: \"Pietre Spezzate\",\n         callback: () => {\n            message = \"Pietre Spezzate\";\n            printMessage(\n               game.tables.contents\n                  .find((t) => t.name === \"Incontro-PietreSpezzate\")\n            );\n\n         },\n      },\n      button6: {\n         label: \"Tana dei Troll\",\n         callback: () => {\n            message = \"Tana dei Troll\";\n            printMessage(\n               game.tables.contents\n                  .find((t) => t.name === \"Incontro-Troll\")\n            );\n         },\n      },\n      button7: {\n         label: \"Vecchia Foresta\",\n         callback: () => {\n            message = \"Vecchia Foresta\";\n            printMessage(\n               game.tables.contents\n                  .find((t) => t.name === \"Incontro-VecchiaForesta\")\n            );\n         },\n      },\n   },\n }).render(true);","flags":{"combat-utility-belt":{"macroTrigger":""},"core":{"sourceId":"Macro.dp35pby9U0TaRMa1"}},"_stats":{"systemId":"tor2e","systemVersion":"2.1.0","coreVersion":"10.291","createdTime":1669988574614,"modifiedTime":1675334624077,"lastModifiedBy":"Gr9xC9PLRip2tzhG"},"folder":null,"sort":0,"ownership":{"default":0,"Gr9xC9PLRip2tzhG":3},"_id":"qVsj5LlZkh4EAL8S"}
